{
 "env": {
  "es6": true
 },
 "extends": ["airbnb-base", "plugin:prettier/recommended"],
 "globals": {
  "Atomics": "readonly",
  "SharedArrayBuffer": "readonly"
 },
 "parserOptions": {
  "ecmaVersion": 11,
  "sourceType": "module"
 },
 "plugins": ["prettier"],
 "rules": {
  "dot-notation": "off",
  "import/no-extraneous-dependencies": "off",
  "no-lone-blocks": "off",
  "no-new-func": "off",
  "no-undef": "off",
  "no-unused-vars": "warn",
  "prettier/prettier": "error"
 },
 "settings": {
  "import/resolver": {
   "webpack": {
    "config": "webpack.config.js"
   }
  }
 }
}
