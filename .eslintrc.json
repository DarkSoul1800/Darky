{
    "env": {
        "es6": true
    },
    "extends": ["standard", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "no-undef": "off",
        "prettier/prettier": "error"
    }
}
